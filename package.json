{
  "name": "reddit-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "fetch-comments": "tsx src/scripts/fetchComments.ts",
    "count-posts": "tsx src/scripts/countPosts.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f postgres",
    "docker:restart": "docker compose restart postgres",
    "db:generate": "dotenvx run -- prisma generate && node scripts/fix-generated-code.js",
    "db:push": "dotenvx run -- prisma db push",
    "db:migrate": "dotenvx run -- prisma migrate dev",
    "db:studio": "dotenvx run -- prisma studio",
    "db:setup": "npm run docker:up && sleep 3 && npm run db:push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Reddit restaurant data scraper with Prisma and tRPC",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@prisma/client": "^6.16.3",
    "@trpc/server": "^11.0.0",
    "csv-parse": "^6.1.0",
    "lodash": "^4.17.21",
    "snoowrap": "^1.23.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "glob": "^11.0.3",
    "prisma": "^6.16.3",
    "prisma-trpc-generator": "^2.4.0",
    "prisma-zod-generator": "^1.26.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}