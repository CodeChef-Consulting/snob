{
  "name": "snob-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f postgres",
    "docker:restart": "docker compose restart postgres",
    "db:generate": "turbo db:generate",
    "db:generate:prod": "cd packages/db && pnpm db:generate:prod",
    "db:push": "turbo db:push",
    "db:push:prod": "cd packages/db && pnpm db:push:prod",
    "db:studio": "cd packages/db && pnpm db:studio",
    "db:studio:prod": "cd packages/db && pnpm db:studio:prod",
    "migrate-to-supabase": "./scripts/migrate-to-supabase.sh"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=18"
  }
}
