{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "dotenvx run -f ../../.env -f ../../.env.keys -- prisma generate && node fix-generated-code.js",
    "db:generate:prod": "dotenvx run -f ../../.env.production -- prisma generate && node fix-generated-code.js",
    "db:push": "dotenvx run -f ../../.env -f ../../.env.keys -- prisma db push",
    "db:push:prod": "dotenvx run -f ../../.env.production -f ../../.env.keys -- prisma db push",
    "db:migrate": "dotenvx run -f ../../.env -f ../../.env.keys -- prisma migrate dev",
    "db:migrate:prod": "dotenvx run -f ../../.env.production -f ../../.env.keys -- prisma migrate dev",
    "db:studio": "dotenvx run -f ../../.env -f ../../.env.keys -- prisma studio",
    "db:studio:prod": "dotenvx run -f ../../.env.production -f ../../.env.keys -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "prisma": "^6.16.3",
    "glob": "^11.0.3",
    "prisma-trpc-generator": "^2.4.0",
    "prisma-zod-generator": "^1.26.0"
  }
}