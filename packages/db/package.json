{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "dotenvx run -f ../../.env -- prisma generate && node fix-generated-code.js",
    "db:push": "dotenvx run -f ../../.env -- prisma db push",
    "db:migrate": "dotenvx run -f ../../.env -- prisma migrate dev",
    "db:studio": "dotenvx run -f ../../.env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "prisma": "^6.16.3",
    "prisma-trpc-generator": "^2.4.0",
    "prisma-zod-generator": "^1.26.0"
  }
}